<%= form_for :photo, url: sitter_photos_path(params[:sitter_id]), class: "form" do |form| %>
    <div class="form-group">
        <%= form.label :photo %>
        <%= form.text_field :photo_url, class: 'form-control' %>
        <%= form.submit 'Upload', class: 'btn btn-b btn-round' %>
    </div>
<% end %> 